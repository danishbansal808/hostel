<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <meta charset="utf-8">
  <title></title>
  <style>
    table, th, td {
    border: 1px solid black;
}
</style>

</head>

<body>
  <table>
    <tr>
      <th>Name</th>
      <th>Block</th>
      <th>Room</th>
      <th>Complaint</th>
      <th>Description</th>
      <th>Recieved</th>
    </tr>
    <%datas.forEach(function(data){%>
    <tr>
      <td>
        <%=data.Name%>
      </td>
      <td>
        <%=data.Block%>
      </td>
      <td>
        <%=data.Room%>
      </td>
      <td>
        <%=data.Complaints%>
      </td>
      <td>
        <%=data.Description%>
      </td>
      <td>
        <button type="button" id="recivebtn" name="btnn" method="post" action="./../assets/managecomplaints/read.js">Mark <s></s>Recieve</button>
      </td>
    </tr>
    <%})%>
  </table>
  <script src="./../assets/js/jquery.js"></script>
  <script src="./../assets/managecomplaints/read.js"></script>
</body>

</html>
